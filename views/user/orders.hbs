<section>
    <div class="container">
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">Date</th>
                    <th>Address</th>
                    <th>Pincode</th>
                    <th>Mobile</th>
                    <th scope="col">Amount</th>
                    <th scope="col">Payment</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>
                {{#each orders}}
                <tr>

                    <td>{{this.date}}</td>
                    <td>{{this.address}}</td>
                    <td>{{this.pincode}}</td>
                    <td>{{this.mobile}}</td>
                    <td>{{this.totalAmount}}</td>
                    <td>{{this.paymentMethod}}</td>
                    <td>{{this.status}}</td>

                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>
